/* Sample blocks problems. */

(def d (x)
	(describe x))

(plan p1
 ; Demonstrate use of unstack action
	(node initial
		(add (clear block1)
		     (ontable block3)
			 (on block1 block2)
			 (on block2 block3)))
	(node goal
		(goal
			(clear block3)))
	(before initial goal)
	)
 
(setq PlanView false)

(setq s (bfs p1))
(setSearchLimit s 100)
;(printf "Call (solve s) now %n")
(setq solutionState (solve s))
(planTree p1)
(setq solution (getProblemState solutionState))
(execute solution)
'done
